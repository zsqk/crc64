{
  "name": "@zsqk/crc64",
  "version": "0.3.3",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts"
  },
  "tasks": {
    "asbuild:debug": "asc assembly/crc64.ts --target debug",
    "asbuild:release": "asc assembly/crc64.ts --target release",
    "asbuild": "deno task asbuild:debug && deno task asbuild:release",
    "prepublish": "deno task asbuild:release && deno run --allow-read --allow-write tools/append-type.ts",
    "test": "deno task asbuild && deno test -A"
  },
  "exclude": [
    "assembly/"
  ],
  "publish": {
    "include": [
      "src/",
      "embed/static/",
      "README.md",
      "LICENSE",
      "deno.json"
    ]
  },
  "imports": {
    "@nfnitloop/deno-embedder": "jsr:@nfnitloop/deno-embedder@^1.6.1",
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@std/path": "jsr:@std/path@^1.1.2",
    "assemblyscript": "npm:assemblyscript@^0.28.8"
  }
}
